    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.80.5",
    "sass-loader": "^16.0.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.10",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-mock": "^3.0.2",
